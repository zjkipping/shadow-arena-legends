{
  "version": 1,
  "projects": {
    "leaderboard": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/leaderboard",
      "sourceRoot": "apps/leaderboard/src",
      "prefix": "sal",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/leaderboard",
            "index": "apps/leaderboard/src/index.html",
            "main": "apps/leaderboard/src/main.ts",
            "polyfills": "apps/leaderboard/src/polyfills.ts",
            "tsConfig": "apps/leaderboard/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/leaderboard/src/favicon.ico",
              "apps/leaderboard/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "apps/leaderboard/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/leaderboard/src/environments/environment.ts",
                  "with": "apps/leaderboard/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "leaderboard:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "leaderboard:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "leaderboard:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/leaderboard/tsconfig.app.json",
              "apps/leaderboard/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/leaderboard/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/leaderboard/jest.config.js",
            "tsConfig": "apps/leaderboard/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/leaderboard/src/test-setup.ts"
          }
        }
      }
    },
    "leaderboard-e2e": {
      "root": "apps/leaderboard-e2e",
